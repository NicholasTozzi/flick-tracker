<div class="header profileHeader">
  <h1>Your Flick Tracker {{username}}</h1>
</div>
<div class="searchMovieLink">
  <style>
    a {
      color: #C10206;
    }
  </style>
  <a href="/review">Search Movie</a>
</div>



  

<div class="all">
    <div class="col profileBoxes">
      {{#if top5.length}}
      
      <div class="blank2">
        <div class="card-header py-3 cf">
        </div>
        {{#each profile as |profile|}}
        <div class="card-body shrek">
          <h4 class="my-0 fw-normal co">&#10026;Top 5&#10026;</h4>
          <ul class="list-unstyled mt-3 mb-4">
            <p>{{profile.top5}}</p>
          </ul>
        </div>
        {{/each}}
        {{else}}
        <div class="card-body shrek">
          <h4 class="my-0 fw-normal co">&#10026;Top 5&#10026;</h4>
          <ul class="list-unstyled mt-3 mb-4">
            <p>Nothing To See Yet!</p>
          </ul>
        </div>
      </div>
    </div>
    {{/if}}
  </div>


    <div class="all">
    <div class="col profileBoxes">
      {{#if profile.watched.length}}
      <div class="blank2">
        <div class="card-header py-3 cf">
        </div>
        {{#each profile as |profile|}}
        <div class="card-body shrek">
          <h4 class="my-0 fw-normal co">&#10026;Recently Watched&#10026;</h4>
          <ul class="list-unstyled mt-3 mb-4">
            <p>{{profile.watched}}</p>
          </ul>
        </div>
        {{/each}}
        {{else}}
        <div class="card-body shrek">
          <h4 class="my-0 fw-normal co">&#10026;Recently Watched&#10026;</h4>
          <ul class="list-unstyled mt-3 mb-4">
            <p>Nothing To See Yet!</p>
          </ul>
        </div>
      </div>
    </div>
    {{/if}}
  </div>

  <div class="all">
    <div class="col profileBoxes">
      {{#if profile.genre.length}}
      <div class="card-header py-3 fiona">
      </div>
      {{#each profile as |profile|}}
      <div class="card-body hi">
        <h4 class="my-0 fw-normal shrek">&#10026;Most Watched Genres&#10026;</h4>
        <p>{{profile.genre}}</p>
       </div>
      {{/each}}
      {{else}}
        <div class="card-header py-3 fiona">
        </div>
     <div class="card-body hi">
        <h4 class="my-0 fw-normal shrek">&#10026;Most Watched Genres&#10026;</h4>
        <p>Nothing To See Yet!</p>   
      </div>
      {{/if}}
    </div>
    </div>

  <div class="all">
  <div class="col profileBoxes">
    {{#if profile.content.length}}
    {{#each profile as |profile|}}
    <div class="card-body partTwo">
      <h5 class="card-title sup">&#10026;Reviews&#10026;</h5>
        <p>{{profile.content}}</p>
          <script src="./javascript/watch.js"></script>
    </div>
    {{/each}}
    {{else}}
       <div class="card-body partTwo">
        <h5 class="card-title sup">&#10026;Reviews&#10026;</h5>
          <p>Nothing To See Yet!</p>
            <script src="./javascript/watch.js"></script>
        </div>
    </div>
      
      {{/if}}
</div>
     