<div class="header profileHeader">
    <h1>Your Flick Tracker</h1>
  </div>
    <div class = "searchMovieLink">
     <style>
      a {
      color: #C10206;
      }
    </style>
    <a href="/review">Search Movie</a>
  </div>
  <div class="profileBody">
      {{#if profile.top5.length}}
    <div class="row row-cols-1 row-cols-md-3 mb-3 text-center yop">
      <div class="col hey">
        <div class="blank">
          <div class="card-header py-3 dv">
           </div>
            {{#each profile as |profile|}}
            <div class="card-body hod">
              <h4 class="my-0 fw-normal co">&#10026;Top 5&#10026;</h4>
                <ul class="list-unstyled mt-3 mb-4 cd">
                  <p>{{profile.top5}}</p>
                </ul>
            </div>
          </div>
        </div>
            {{/each}}
            {{/if}}

      
        <div class="col hey">
          {{#if profile.watched.length}}
          <div class="blank2">
            <div class="card-header py-3 cf">
            </div>
            {{#each profile as |profile|}}
            <div class="card-body shrek">
              <h4 class="my-0 fw-normal co">&#10026;Recently Watched&#10026;</h4>
                <ul class="list-unstyled mt-3 mb-4">
                <p>{{profile.watched}}</p>
              </ul>
            </div>
          </div>
        </div>
        {{/each}}
        {{/if}}
        
        <div class="col hey">
          {{#if profile.genre.length}}
            <div class="card-header py-3 fiona">
            </div>
            {{#each profile as |profile|}}
              <div class="card-body hi">
                <h4 class="my-0 fw-normal shrek">&#10026;Most Watched Genres&#10026;</h4>
                <p>{{profile.genre}}</p>
              <ul class="list-unstyled mt-3 mb-4 donkey">
            </div>
          </div>
         </div>
         {{/each}}
         {{/if}}
      
  <div class="card5 reviewBody">
    {{#if profile.content.length}}
    <div class="card-body partTwo">
      {{#each profile as |profile|}}
      <h5 class="card-title sup">&#10026;Reviews&#10026;</h5>
        <p>{{profile.content}}</p>
        <script src="/public/javascript/watch.js"></script>
          </div>
        </div>
       </div>
    </div>
      {{/each}}
      {{/if}}