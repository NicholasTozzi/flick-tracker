<div class="header community">
  <h1>The Flick Tracker Community<br /></h1>
</div>
<div class="bulks">
  <div class="card text-center 12 shrek">
    {{#each allReviews as |review|}}
      <div class="card-body communityBody">
        {{! <h4 style="text-align:left;"> }}
        <h4>
          {{review.name}}
          <span style="float:right;">
            {{format_date date_created}}
          </span>
        </h4>
        <p class="card-text hey">{{review.content}} - {{user.username}}</p>
        {{! <div class="btn-group jesse" role="group" aria-label="Basic outlined example">
        <button type="button" class="btn btn-outline-primary saveToWatchlist" id="#already-watched">Save To Watchlist</button>
        <button type="button" class="btn btn-outline-primary alreadyWatched" id="#watch-later">Already Watched</button>
      </div> }}
      </div>
    {{/each}}
  </div>
</div>