<div class="header community">
  <h1>The Flick Tracker Community<br></h1>
</div>
{{#if reviews.length}}
<div class="bulks">
  <div class="card text-center 12 shrek">
    {{#each review as |review|}}
    <div class="card-body communityBody">
      <h4 style="text-align:left;">
        {{review.movie}}
        <span style="float:right;">
          {{review.date_created}}
        </span>
      </h4>
      <p class="card-text hey">{{review.content}} - {{review.user_id}}</p>
      <div class="btn-group jesse" role="group" aria-label="Basic outlined example">
        <button type="button" class="btn btn-outline-primary saveToWatchlist">Save To Watchlist</button>
        <button type="button" class="btn btn-outline-primary alreadyWatched">Already Watched</button>
      </div>
    </div>
  </div>
  {{else}}
  <h1>Nothing To See Yet</h1>
  {{/each}}
</div>
{{/if}}